<div>
  <h2>Here are the locations in the map</h2>
  <% for (const location of locations) { %>
    <form class="location-button" id="loc_<%=location.location_id%>" action="/location/<%= location.location_id %>" method="get">
      <input location-id="<%=location.location_id%>" type="submit" value="<%= location.location_name %>"></input>
    </form>
    <form id="delete-location" location-id="<%=location.location_id%>" action="/location/<%=location.location_id%>/delete" method="post">
      <input type="submit" value="delete">
    </form>
    <form class="like-button" location-id="<%=location.location_id%>" action="/location/<%= location.location_id%>/like" method="post">
      <input type="submit" value="Like">
    </form>
  <% } %>
</div>


